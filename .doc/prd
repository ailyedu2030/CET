if [ $API_CHECK_RESULT -eq 0 ]; then
    echo "  åç«¯APIä¸€è‡´æ€§: âœ… é€šè¿‡"
else
    echo "  åç«¯APIä¸€è‡´æ€§: âŒ å¤±è´¥"
fi

if [ $FRONTEND_TYPE_CHECK -eq 0 ]; then
    echo "  å‰ç«¯ç±»å‹æ£€æŸ¥: âœ… é€šè¿‡"
else
    echo "  å‰ç«¯ç±»å‹æ£€æŸ¥: âŒ å¤±è´¥"
fi

if [ $FRONTEND_API_TEST -eq 0 ]; then
    echo "  å‰ç«¯APIé›†æˆæµ‹è¯•: âœ… é€šè¿‡"
else
    echo "  å‰ç«¯APIé›†æˆæµ‹è¯•: âŒ å¤±è´¥"
fi

# 9. åˆ¤æ–­æ€»ä½“ç»“æœ
if [ $API_CHECK_RESULT -eq 0 ] && [ $FRONTEND_TYPE_CHECK -eq 0 ] && [ $FRONTEND_API_TEST -eq 0 ]; then
    echo "ğŸ‰ APIå¯¹é½éªŒè¯å…¨éƒ¨é€šè¿‡ï¼å¯ä»¥ç»§ç»­å¼€å‘"
    exit 0
else
    echo "ğŸš¨ APIå¯¹é½éªŒè¯å¤±è´¥ï¼Claude 4.0å¿…é¡»ä¿®å¤æ‰€æœ‰é—®é¢˜åæ‰èƒ½ç»§ç»­"
    exit 1
fi
```

#### ğŸ”’ ç­–ç•¥5ï¼šå®æ—¶APIæ–‡æ¡£åŒæ­¥
**ç¡®ä¿å¼€å‘è¿‡ç¨‹ä¸­æ–‡æ¡£å’Œä»£ç å§‹ç»ˆåŒæ­¥**

```python
# tools/api_doc_sync.py
"""
å®æ—¶APIæ–‡æ¡£åŒæ­¥å·¥å…·
ç¡®ä¿OpenAPIè§„èŒƒã€åç«¯ä»£ç ã€å‰ç«¯ä»£ç ä¸‰è€…ä¿æŒåŒæ­¥
"""

import os
import re
import yaml
from typing import Dict, List

class APIDocSync:
    def __init__(self):
        self.spec_file = 'api-specification.yaml'
        self.backend_dir = 'backend/apps'
        self.frontend_api_dir = 'frontend/src/api'
        
    def sync_all(self):
        """åŒæ­¥æ‰€æœ‰APIæ–‡æ¡£å’Œä»£ç """
        print("ğŸ”„ å¼€å§‹APIæ–‡æ¡£åŒæ­¥...")
        
        # 1. ä»OpenAPIè§„èŒƒç”Ÿæˆåç«¯ä»£ç éª¨æ¶
        self.generate_backend_skeleton()
        
        # 2. ä»OpenAPIè§„èŒƒç”Ÿæˆå‰ç«¯TypeScriptç±»å‹
        self.generate_frontend_types()
        
        # 3. ä»OpenAPIè§„èŒƒç”Ÿæˆå‰ç«¯APIå®¢æˆ·ç«¯
        self.generate_frontend_client()
        
        print("âœ… APIæ–‡æ¡£åŒæ­¥å®Œæˆ!")
    
    def generate_backend_skeleton(self):
        """ä»OpenAPIè§„èŒƒç”ŸæˆDjangoåç«¯ä»£ç éª¨æ¶"""
        spec = self.load_spec()
        
        for path, methods in spec['paths'].items():
            # è§£æè·¯å¾„ï¼Œç¡®å®šåº”ç”¨å’Œè§†å›¾
            app_name = self.extract_app_name(path)
            view_name = self.extract_view_name(path)
            
            # ç”Ÿæˆè§†å›¾ä»£ç éª¨æ¶
            self.generate_django_view(app_name, view_name, path, methods)
    
    def generate_frontend_types(self):
        """ç”ŸæˆTypeScriptç±»å‹å®šä¹‰"""
        spec = self.load_spec()
        
        types_content = "// Auto-generated TypeScript types from OpenAPI spec\n\n"
        
        # ç”Ÿæˆæ¥å£ç±»å‹
        if 'components' in spec and 'schemas' in spec['components']:
            for schema_name, schema in spec['components']['schemas'].items():
                types_content += self.generate_ts_interface(schema_name, schema)
        
        # å†™å…¥æ–‡ä»¶
        os.makedirs(self.frontend_api_dir, exist_ok=True)
        with open(f"{self.frontend_api_dir}/types.ts", 'w', encoding='utf-8') as f:
            f.write(types_content)
    
    def generate_frontend_client(self):
        """ç”Ÿæˆå‰ç«¯APIå®¢æˆ·ç«¯ä»£ç """
        spec = self.load_spec()
        
        client_content = """// Auto-generated API client from OpenAPI spec
import { ApiResponse, RequestConfig } from './types';

export class APIClient {
  private baseURL: string;
  
  constructor(baseURL: string = process.env.NEXT_PUBLIC_API_URL || 'http://localhost:8000') {
    this.baseURL = baseURL;
  }
  
  private async request<T>(config: RequestConfig): Promise<T> {
    const response = await fetch(`${this.baseURL}${config.url}`, {
      method: config.method,
      headers: {
        'Content-Type': 'application/json',
        ...config.headers
      },
      body: config.body ? JSON.stringify(config.body) : undefined
    });
    
    if (!response.ok) {
      throw new Error(`API Error: ${response.statusText}`);
    }
    
    return response.json();
  }

"""
        
        # ä¸ºæ¯ä¸ªAPIç«¯ç‚¹ç”Ÿæˆæ–¹æ³•
        for path, methods in spec['paths'].items():
            for method, operation in methods.items():
                client_content += self.generate_client_method(path, method, operation)
        
        client_content += "}\n"
        
        # å†™å…¥æ–‡ä»¶
        with open(f"{self.frontend_api_dir}/client.ts", 'w', encoding='utf-8') as f:
            f.write(client_content)

### ğŸ“Š APIå¯¹é½éªŒè¯ä»ªè¡¨æ¿
**å®æ—¶ç›‘æ§APIå¯¹é½çŠ¶æ€**

```typescript
// frontend/src/components/ApiAlignmentDashboard.tsx
/**
 * APIå¯¹é½éªŒè¯ä»ªè¡¨æ¿
 * å¼€å‘æœŸé—´å®æ—¶æ˜¾ç¤ºAPIå¯¹é½çŠ¶æ€
 */

import React, { useState, useEffect } from 'react';
import { Badge } from '@/components/ui/badge';
import { Table, TableBody, TableCell, TableHead, TableHeader, TableRow } from '@/components/ui/table';

interface ApiStatus {
  endpoint: string;
  method: string;
  status: 'aligned' | 'misaligned' | 'error';
  lastCheck: string;
  errors?: string[];
}

export const ApiAlignmentDashboard: React.FC = () => {
  const [apiStatuses, setApiStatuses] = useState<ApiStatus[]>([]);
  const [overallStatus, setOverallStatus] = useState<'good' | 'warning' | 'error'>('good');

  useEffect(() => {
    // å®šæœŸæ£€æŸ¥APIå¯¹é½çŠ¶æ€
    const checkAlignment = async () => {
      try {
        const response = await fetch('/api/dev/alignment-check');
        const data = await response.json();
        setApiStatuses(data.statuses);
        setOverallStatus(data.overall_status);
      } catch (error) {
        console.error('Failed to check API alignment:', error);
      }
    };

    checkAlignment();
    const interval = setInterval(checkAlignment, 30000); // æ¯30ç§’æ£€æŸ¥ä¸€æ¬¡

    return () => clearInterval(interval);
  }, []);

  const getStatusColor = (status: string) => {
    switch (status) {
      case 'aligned': return 'green';
      case 'misaligned': return 'orange';
      case 'error': return 'red';
      default: return 'gray';
    }
  };

  return (
    <Box p={6} bg="white" borderRadius="lg" shadow="md">
      <Box mb={4}>
        <Badge 
          colorScheme={getStatusColor(overallStatus)} 
          fontSize="lg" 
          p={2}
        >
          APIå¯¹é½çŠ¶æ€: {overallStatus.toUpperCase()}
        </Badge>
      </Box>

      <Table variant="simple">
        <Thead>
          <Tr>
            <Th>APIç«¯ç‚¹</Th>
            <Th>æ–¹æ³•</Th>
            <Th>çŠ¶æ€</Th>
            <Th>æœ€åæ£€æŸ¥</Th>
            <Th>é”™è¯¯ä¿¡æ¯</Th>
          </Tr>
        </Thead>
        <Tbody>
          {apiStatuses.map((api, index) => (
            <Tr key={index}>
              <Td>{api.endpoint}</Td>
              <Td>{api.method.toUpperCase()}</Td>
              <Td>
                <Badge colorScheme={getStatusColor(api.status)}>
                  {api.status}
                </Badge>
              </Td>
              <Td>{api.lastCheck}</Td>
              <Td>
                {api.errors && api.errors.length > 0 ? (
                  <Box>
                    {api.errors.map((error, i) => (
                      <div key={i} style={{ fontSize: '12px', color: 'red' }}>
                        {error}
                      </div>
                    ))}
                  </Box>
                ) : (
                  'æ— '
                )}
              </Td>
            </Tr>
          ))}
        </Tbody>
      </Table>
    </Box>
  );
};
```

### âœ… Claude 4.0å¼€å‘å·¥ä½œæµå¼ºåˆ¶æ£€æŸ¥ç‚¹

**Task 1ï¼ˆåç«¯å¼€å‘ï¼‰å®Œæˆæ¡ä»¶**ï¼š
1. âœ… åˆ›å»ºå®Œæ•´OpenAPIè§„èŒƒæ–‡ä»¶ï¼ˆ150+ä¸ªAPIç«¯ç‚¹ï¼‰
2. âœ… åç«¯APIå®ç°100%ç¬¦åˆOpenAPIè§„èŒƒ
3. âœ… APIä¸€è‡´æ€§æ£€æŸ¥100%é€šè¿‡
4. âœ… è‡ªåŠ¨åŒ–æµ‹è¯•è¦†ç›–æ‰€æœ‰APIç«¯ç‚¹

**Task 2ï¼ˆå‰ç«¯å¼€å‘ï¼‰å®Œæˆæ¡ä»¶**ï¼š
1. âœ… TypeScriptç±»å‹å®šä¹‰ä¸OpenAPIè§„èŒƒ100%ä¸€è‡´
2. âœ… APIå®¢æˆ·ç«¯ä»£ç ä¸åç«¯API 100%å¯¹é½
3. âœ… å‰ç«¯APIé›†æˆæµ‹è¯•100%é€šè¿‡
4. âœ… å®æ—¶APIå¯¹é½éªŒè¯ä»ªè¡¨æ¿æ­£å¸¸å·¥ä½œ

**æ¯æ—¥å¼€å‘æ£€æŸ¥æ¸…å•**ï¼š
- [ ] è¿è¡Œ `./verify_api_alignment.sh` é€šè¿‡
- [ ] APIæ–‡æ¡£åŒæ­¥å·¥å…·è¿è¡Œæ— é”™è¯¯
- [ ] å‰åç«¯é›†æˆæµ‹è¯•å…¨éƒ¨é€šè¿‡
- [ ] APIå¯¹é½ä»ªè¡¨æ¿æ˜¾ç¤ºç»¿è‰²çŠ¶æ€

### ğŸ¯ æˆåŠŸä¿éšœæŒ‡æ ‡
- **APIå¯¹é½ç‡**: 100%ï¼ˆ150+ä¸ªAPIç«¯ç‚¹å…¨éƒ¨å¯¹é½ï¼‰
- **ç±»å‹å®‰å…¨**: 100%ï¼ˆTypeScriptä¸¥æ ¼æ¨¡å¼é€šè¿‡ï¼‰
- **é›†æˆæµ‹è¯•é€šè¿‡ç‡**: 100%
- **æ–‡æ¡£åŒæ­¥ç‡**: 100%ï¼ˆä»£ç ä¸æ–‡æ¡£å®æ—¶åŒæ­¥ï¼‰

**é€šè¿‡è¿™å¥—æœºåˆ¶ï¼ŒClaude 4.0åœ¨å¼€å‘è¿‡ç¨‹ä¸­å°†è¢«å¼ºåˆ¶ä¿æŒå‰åç«¯APIçš„100%ä¸€è‡´æ€§ï¼Œå®Œå…¨æ¶ˆé™¤APIä¸åŒ¹é…çš„é£é™©ï¼**

## ğŸ›¡ï¸ æ™ºèƒ½ä½“å¼€å‘å…¨é£é™©é˜²èŒƒä½“ç³»ï¼ˆ10å¤§é£é™©å…¨è¦†ç›–ï¼‰

### ğŸ¯ æ ¸å¿ƒç›®æ ‡
**æ„å»ºå…¨æ–¹ä½é£é™©é˜²èŒƒä½“ç³»ï¼Œå°†æ™ºèƒ½ä½“å¼€å‘æˆåŠŸç‡ä»99%æå‡åˆ°99.9%ï¼Œå®ç°çœŸæ­£çš„"ä¸‡æ— ä¸€å¤±"**

### ğŸš¨ æ™ºèƒ½ä½“å¼€å‘10å¤§é£é™©åˆ†æ

#### ğŸ§  **é£é™©1ï¼šä¸Šä¸‹æ–‡ä¸¢å¤±é£é™©**
**é—®é¢˜æè¿°**ï¼š
- æ™ºèƒ½ä½“åœ¨é•¿æœŸå¼€å‘ä¸­é—å¿˜ä¹‹å‰çš„ä»£ç ç»“æ„å’Œè®¾è®¡å†³ç­–
- å¤æ‚é¡¹ç›®ä¸­éš¾ä»¥è®°ä½æ‰€æœ‰æ¨¡å—é—´çš„ä¾èµ–å…³ç³»
- å¯èƒ½é‡å¤åˆ›å»ºå·²å­˜åœ¨çš„åŠŸèƒ½æˆ–äº§ç”Ÿå†²çª

**é˜²èŒƒæœºåˆ¶**ï¼š
```python
# tools/context_manager.py
"""
æ™ºèƒ½ä½“ä¸Šä¸‹æ–‡ç®¡ç†ç³»ç»Ÿ
è‡ªåŠ¨ä¿å­˜å’Œæ¢å¤é¡¹ç›®ä¸Šä¸‹æ–‡ä¿¡æ¯
"""

import json
import os
from datetime import datetime
from typing import Dict, List

class ContextManager:
    def __init__(self, project_root: str):
        self.project_root = project_root
        self.context_file = f"{project_root}/.taskmaster/context/project_context.json"
        self.snapshots_dir = f"{project_root}/.taskmaster/context/snapshots"
        os.makedirs(os.path.dirname(self.context_file), exist_ok=True)
        os.makedirs(self.snapshots_dir, exist_ok=True)
    
    def save_context(self, context_data: Dict):
        """ä¿å­˜å½“å‰é¡¹ç›®ä¸Šä¸‹æ–‡"""
        context = {
            'timestamp': datetime.now().isoformat(),
            'project_structure': self.scan_project_structure(),
            'api_endpoints': self.extract_api_endpoints(),
            'database_tables': self.extract_database_schema(),
            'dependencies': self.extract_dependencies(),
            'key_decisions': context_data.get('decisions', []),
            'module_relationships': self.analyze_module_dependencies(),
            'current_phase': context_data.get('phase'),
            'completed_features': context_data.get('completed_features', []),
            'next_steps': context_data.get('next_steps', [])
        }
        
        with open(self.context_file, 'w', encoding='utf-8') as f:
            json.dump(context, f, indent=2, ensure_ascii=False)
        
        # åˆ›å»ºå¿«ç…§
        snapshot_file = f"{self.snapshots_dir}/context_{datetime.now().strftime('%Y%m%d_%H%M%S')}.json"
        with open(snapshot_file, 'w', encoding='utf-8') as f:
            json.dump(context, f, indent=2, ensure_ascii=False)
    
    def load_context(self) -> Dict:
        """åŠ è½½é¡¹ç›®ä¸Šä¸‹æ–‡"""
        if os.path.exists(self.context_file):
            with open(self.context_file, 'r', encoding='utf-8') as f:
                return json.load(f)
        return {}
    
    def get_context_summary(self) -> str:
        """è·å–ä¸Šä¸‹æ–‡æ‘˜è¦ä¾›æ™ºèƒ½ä½“å‚è€ƒ"""
        context = self.load_context()
        if not context:
            return "é¡¹ç›®ä¸Šä¸‹æ–‡ä¸ºç©ºï¼Œè¿™æ˜¯æ–°é¡¹ç›®çš„å¼€å§‹ã€‚"
        
        summary = f"""
é¡¹ç›®ä¸Šä¸‹æ–‡æ‘˜è¦ (æ›´æ–°æ—¶é—´: {context.get('timestamp', 'Unknown')})

ğŸ“ é¡¹ç›®ç»“æ„:
{self.format_project_structure(context.get('project_structure', {}))}

ğŸ”— APIç«¯ç‚¹: {len(context.get('api_endpoints', []))}ä¸ª
ğŸ“Š æ•°æ®è¡¨: {len(context.get('database_tables', []))}å¼ 
ğŸ“¦ ä¾èµ–: {len(context.get('dependencies', []))}ä¸ª

âœ… å·²å®ŒæˆåŠŸèƒ½:
{chr(10).join([f"- {feature}" for feature in context.get('completed_features', [])])}

ğŸ“‹ ä¸‹ä¸€æ­¥è®¡åˆ’:
{chr(10).join([f"- {step}" for step in context.get('next_steps', [])])}

ğŸ¯ å½“å‰é˜¶æ®µ: {context.get('current_phase', 'Unknown')}
"""
        return summary
```

#### ğŸ’ **é£é™©2ï¼šä»£ç è´¨é‡é£é™©**
**é—®é¢˜æè¿°**ï¼š
- æ™ºèƒ½ä½“ç”Ÿæˆçš„ä»£ç å¯èƒ½ä¸ç¬¦åˆæœ€ä½³å®è·µ
- ç¼ºä¹ä¸€è‡´çš„ä»£ç é£æ ¼å’Œå‘½åè§„èŒƒ
- å­˜åœ¨å®‰å…¨æ¼æ´æˆ–æ€§èƒ½é—®é¢˜

**é˜²èŒƒæœºåˆ¶**ï¼š
```bash
#!/bin/bash
# tools/code_quality_gate.sh
# ä»£ç è´¨é‡é—¨ç¦ç³»ç»Ÿ

echo "ğŸ” å¼€å§‹ä»£ç è´¨é‡æ£€æŸ¥..."

# 1. Pythonä»£ç è´¨é‡æ£€æŸ¥
echo "ğŸ“ æ£€æŸ¥Pythonä»£ç è´¨é‡..."
cd backend
pylint **/*.py --fail-under=8.0
if [ $? -ne 0 ]; then
    echo "âŒ Pythonä»£ç è´¨é‡ä¸è¾¾æ ‡ (è¦æ±‚>=8.0)"
    exit 1
fi

bandit -r . -f json -o security_report.json
if [ $? -ne 0 ]; then
    echo "âŒ Pythonå®‰å…¨æ‰«æå‘ç°é—®é¢˜"
    exit 1
fi

# 2. JavaScript/TypeScriptä»£ç è´¨é‡æ£€æŸ¥
echo "ğŸ“ æ£€æŸ¥å‰ç«¯ä»£ç è´¨é‡..."
cd ../frontend
npx eslint src/ --ext .ts,.tsx --max-warnings 0
if [ $? -ne 0 ]; then
    echo "âŒ å‰ç«¯ä»£ç è´¨é‡ä¸è¾¾æ ‡"
    exit 1
fi

# 3. ä»£ç å¤æ‚åº¦æ£€æŸ¥
npx plato -r -d complexity_report src/
complexity_score=$(cat complexity_report/report.json | jq '.summary.average.complexity')
if (( $(echo "$complexity_score > 10" | bc -l) )); then
    echo "âŒ ä»£ç å¤æ‚åº¦è¿‡é«˜: $complexity_score (è¦æ±‚<=10)"
    exit 1
fi

# 4. ä»£ç é‡å¤æ£€æŸ¥
npx jscpd src/ --threshold 5
if [ $? -ne 0 ]; then
    echo "âŒ ä»£ç é‡å¤ç‡è¿‡é«˜"
    exit 1
fi

echo "âœ… ä»£ç è´¨é‡æ£€æŸ¥å…¨éƒ¨é€šè¿‡!"
```

#### ğŸ”— **é£é™©3ï¼šä¾èµ–ç®¡ç†é£é™©**
**é—®é¢˜æè¿°**ï¼š
- ç‰ˆæœ¬å†²çªå¯¼è‡´é¡¹ç›®æ— æ³•è¿è¡Œ
- å¾ªç¯ä¾èµ–å’Œæœªå£°æ˜ä¾èµ–
- å®‰å…¨æ¼æ´ä¾èµ–

**é˜²èŒƒæœºåˆ¶**ï¼š
```python
# tools/dependency_analyzer.py
"""
ä¾èµ–åˆ†æå’Œç®¡ç†å·¥å…·
è‡ªåŠ¨æ£€æµ‹ä¾èµ–é—®é¢˜å¹¶æä¾›è§£å†³æ–¹æ¡ˆ
"""

import json
import subprocess
import networkx as nx
from typing import Dict, List

class DependencyAnalyzer:
    def __init__(self, project_root: str):
        self.project_root = project_root
        
    def check_python_dependencies(self) -> Dict:
        """æ£€æŸ¥Pythonä¾èµ–"""
        results = {
            'conflicts': [],
            'security_issues': [],
            'outdated': [],
            'circular_deps': []
        }
        
        # æ£€æŸ¥ä¾èµ–å†²çª
        try:
            result = subprocess.run(['pip', 'check'], capture_output=True, text=True)
            if result.returncode != 0:
                results['conflicts'] = result.stdout.split('\n')
        except Exception as e:
            results['conflicts'] = [f"æ£€æŸ¥å¤±è´¥: {str(e)}"]
        
        # å®‰å…¨æ¼æ´æ£€æŸ¥
        try:
            result = subprocess.run(['safety', 'check', '--json'], capture_output=True, text=True)
            if result.returncode != 0:
                results['security_issues'] = json.loads(result.stdout)
        except Exception:
            pass
        
        # è¿‡æœŸä¾èµ–æ£€æŸ¥
        try:
            result = subprocess.run(['pip', 'list', '--outdated', '--format=json'], capture_output=True, text=True)
            results['outdated'] = json.loads(result.stdout)
        except Exception:
            pass
        
        return results
    
    def check_node_dependencies(self) -> Dict:
        """æ£€æŸ¥Node.jsä¾èµ–"""
        results = {
            'security_issues': [],
            'outdated': []
        }
        
        # å®‰å…¨æ£€æŸ¥
        try:
            result = subprocess.run(['npm', 'audit', '--json'], capture_output=True, text=True)
            audit_data = json.loads(result.stdout)
            if audit_data.get('vulnerabilities'):
                results['security_issues'] = audit_data['vulnerabilities']
        except Exception:
            pass
        
        # è¿‡æœŸä¾èµ–æ£€æŸ¥
        try:
            result = subprocess.run(['npm', 'outdated', '--json'], capture_output=True, text=True)
            if result.stdout:
                results['outdated'] = json.loads(result.stdout)
        except Exception:
            pass
        
        return results
    
    def detect_circular_dependencies(self) -> List:
        """æ£€æµ‹å¾ªç¯ä¾èµ–"""
        graph = nx.DiGraph()
        
        # æ‰«æimportè¯­å¥æ„å»ºä¾èµ–å›¾
        # (ç®€åŒ–ç‰ˆæœ¬ï¼Œå®é™…å®ç°éœ€è¦è§£ææ‰€æœ‰Pythonæ–‡ä»¶)
        
        try:
            cycles = list(nx.simple_cycles(graph))
            return cycles
        except Exception:
            return []
    
    def generate_dependency_report(self) -> Dict:
        """ç”Ÿæˆå®Œæ•´çš„ä¾èµ–æŠ¥å‘Š"""
        report = {
            'timestamp': datetime.now().isoformat(),
            'python': self.check_python_dependencies(),
            'node': self.check_node_dependencies(),
            'circular_deps': self.detect_circular_dependencies()
        }
        
        # è®¡ç®—é£é™©è¯„åˆ†
        risk_score = 0
        if report['python']['conflicts']:
            risk_score += 30
        if report['python']['security_issues']:
            risk_score += 40
        if report['node']['security_issues']:
            risk_score += 40
        if report['circular_deps']:
            risk_score += 20
        
        report['risk_score'] = min(risk_score, 100)
        report['risk_level'] = 'High' if risk_score > 70 else 'Medium' if risk_score > 30 else 'Low'
        
        return report
```

#### ğŸ—„ï¸ **é£é™©4ï¼šæ•°æ®åº“è®¾è®¡é£é™©**
**é—®é¢˜æè¿°**ï¼š
- è¡¨ç»“æ„è®¾è®¡ä¸åˆç†ï¼Œå½±å“æ€§èƒ½
- ç¼ºå°‘å¿…è¦çš„ç´¢å¼•
- æ•°æ®è¿ç§»è„šæœ¬é”™è¯¯

**é˜²èŒƒæœºåˆ¶**ï¼š
```python
# tools/database_analyzer.py
"""
æ•°æ®åº“è®¾è®¡åˆ†æå’Œä¼˜åŒ–å»ºè®®å·¥å…·
"""

from sqlalchemy import create_engine, inspect
from typing import Dict, List

class DatabaseAnalyzer:
    def __init__(self, database_url: str):
        self.engine = create_engine(database_url)
        self.inspector = inspect(self.engine)
    
    def analyze_table_design(self) -> Dict:
        """åˆ†æè¡¨è®¾è®¡åˆç†æ€§"""
        analysis = {
            'tables': [],
            'issues': [],
            'suggestions': []
        }
        
        for table_name in self.inspector.get_table_names():
            columns = self.inspector.get_columns(table_name)
            indexes = self.inspector.get_indexes(table_name)
            foreign_keys = self.inspector.get_foreign_keys(table_name)
            
            table_info = {
                'name': table_name,
                'columns': len(columns),
                'indexes': len(indexes),
                'foreign_keys': len(foreign_keys),
                'issues': []
            }
            
            # æ£€æŸ¥è®¾è®¡é—®é¢˜
            # 1. ç¼ºå°‘ä¸»é”®
            primary_keys = [col for col in columns if col.get('primary_key')]
            if not primary_keys:
                table_info['issues'].append('ç¼ºå°‘ä¸»é”®')
                analysis['issues'].append(f"{table_name}: ç¼ºå°‘ä¸»é”®")
            
            # 2. è¿‡å¤šåˆ—ï¼ˆå¯èƒ½éœ€è¦æ‹†åˆ†è¡¨ï¼‰
            if len(columns) > 20:
                table_info['issues'].append('åˆ—æ•°è¿‡å¤šï¼Œå»ºè®®æ‹†åˆ†è¡¨')
                analysis['suggestions'].append(f"{table_name}: è€ƒè™‘æ‹†åˆ†è¡¨ï¼Œå½“å‰{len(columns)}åˆ—")
            
            # 3. ç¼ºå°‘ç´¢å¼•
            non_pk_columns = [col for col in columns if not col.get('primary_key')]
            if len(non_pk_columns) > 5 and len(indexes) < 2:
                table_info['issues'].append('å¯èƒ½ç¼ºå°‘ç´¢å¼•')
                analysis['suggestions'].append(f"{table_name}: å»ºè®®æ·»åŠ ç´¢å¼•")
            
            analysis['tables'].append(table_info)
        
        return analysis
    
    def suggest_indexes(self) -> List[Dict]:
        """åŸºäºæŸ¥è¯¢æ¨¡å¼å»ºè®®ç´¢å¼•"""
        suggestions = []
        
        # è¿™é‡Œéœ€è¦åˆ†ææŸ¥è¯¢æ—¥å¿—æˆ–ORMæŸ¥è¯¢
        # ç®€åŒ–ç‰ˆæœ¬ï¼šåŸºäºå¤–é”®å»ºè®®ç´¢å¼•
        for table_name in self.inspector.get_table_names():
            foreign_keys = self.inspector.get_foreign_keys(table_name)
            existing_indexes = self.inspector.get_indexes(table_name)
            
            indexed_columns = set()
            for idx in existing_indexes:
                indexed_columns.update(idx['column_names'])
            
            for fk in foreign_keys:
                for col in fk['constrained_columns']:
                    if col not in indexed_columns:
                        suggestions.append({
                            'table': table_name,
                            'column': col,
                            'type': 'foreign_key_index',
                            'sql': f"CREATE INDEX idx_{table_name}_{col} ON {table_name}({col});"
                        })
        
        return suggestions
```

#### ğŸ§ª **é£é™©5ï¼šæµ‹è¯•è¦†ç›–é£é™©**
**é—®é¢˜æè¿°**ï¼š
- æµ‹è¯•ç”¨ä¾‹ä¸å®Œæ•´ï¼Œå…³é”®åŠŸèƒ½ç¼ºå°‘æµ‹è¯•
- æµ‹è¯•æ•°æ®ä¸çœŸå®ï¼Œæ— æ³•å‘ç°å®é™…é—®é¢˜

**é˜²èŒƒæœºåˆ¶**ï¼š
```python
# tools/test_coverage_analyzer.py
"""
æµ‹è¯•è¦†ç›–ç‡åˆ†æå’Œå¼ºåˆ¶è¦æ±‚å·¥å…·
"""

import coverage
import json
from typing import Dict, List

class TestCoverageAnalyzer:
    def __init__(self, project_root: str):
        self.project_root = project_root
        self.cov = coverage.Coverage()
    
    def analyze_coverage(self) -> Dict:
        """åˆ†ææµ‹è¯•è¦†ç›–ç‡"""
        self.cov.start()
        
        # è¿è¡Œæµ‹è¯•
        import subprocess
        subprocess.run(['python', '-m', 'pytest', 'tests/', '-v'], cwd=self.project_root)
        
        self.cov.stop()
        self.cov.save()
        
        # ç”Ÿæˆè¦†ç›–ç‡æŠ¥å‘Š
        report = self.cov.report(show_missing=True)
        
        # è·å–è¯¦ç»†ä¿¡æ¯
        analysis = {
            'overall_coverage': self.cov.report(),
            'uncovered_lines': {},
            'critical_paths_missing': [],
            'suggestions': []
        }
        
        # æ£€æŸ¥å…³é”®è·¯å¾„è¦†ç›–
        critical_files = [
            'views.py', 'models.py', 'serializers.py',
            'api.py', 'auth.py', 'payment.py'
        ]
        
        for filename in critical_files:
            file_coverage = self.get_file_coverage(filename)
            if file_coverage < 80:
                analysis['critical_paths_missing'].append({
                    'file': filename,
                    'coverage': file_coverage,
                    'required': 80
                })
        
        return analysis
    
    def generate_test_requirements(self) -> List[Dict]:
        """ç”Ÿæˆæµ‹è¯•è¦æ±‚æ¸…å•"""
        requirements = [
            {
                'category': 'APIæµ‹è¯•',
                'tests': [
                    'æ‰€æœ‰APIç«¯ç‚¹çš„æˆåŠŸå“åº”æµ‹è¯•',
                    'é”™è¯¯å¤„ç†å’Œå¼‚å¸¸æƒ…å†µæµ‹è¯•',
                    'æƒé™å’Œè®¤è¯æµ‹è¯•',
                    'è¾“å…¥éªŒè¯æµ‹è¯•'
                ],
                'min_coverage': 90
            },
            {
                'category': 'ä¸šåŠ¡é€»è¾‘æµ‹è¯•',
                'tests': [
                    'æ ¸å¿ƒä¸šåŠ¡æµç¨‹æµ‹è¯•',
                    'è¾¹ç•Œæ¡ä»¶æµ‹è¯•',
                    'æ•°æ®ä¸€è‡´æ€§æµ‹è¯•'
                ],
                'min_coverage': 85
            },
            {
                'category': 'é›†æˆæµ‹è¯•',
                'tests': [
                    'æ•°æ®åº“é›†æˆæµ‹è¯•',
                    'å¤–éƒ¨APIé›†æˆæµ‹è¯•',
                    'ç«¯åˆ°ç«¯ç”¨æˆ·æµç¨‹æµ‹è¯•'
                ],
                'min_coverage': 70
            }
        ]
        
        return requirements
```

#### âš™ï¸ **é£é™©6ï¼šç¯å¢ƒé…ç½®é£é™©**
**é—®é¢˜æè¿°**ï¼š
- å¼€å‘ã€æµ‹è¯•ã€ç”Ÿäº§ç¯å¢ƒä¸ä¸€è‡´
- ç¯å¢ƒå˜é‡ç®¡ç†æ··ä¹±

**é˜²èŒƒæœºåˆ¶**ï¼š
```bash
#!/bin/bash
# tools/environment_validator.sh
# ç¯å¢ƒä¸€è‡´æ€§éªŒè¯å·¥å…·

echo "ğŸ”§ å¼€å§‹ç¯å¢ƒä¸€è‡´æ€§éªŒè¯..."

# 1. æ£€æŸ¥Pythonç‰ˆæœ¬
PYTHON_VERSION=$(python --version 2>&1 | cut -d' ' -f2)
REQUIRED_PYTHON="3.11"
if [[ "$PYTHON_VERSION" != "$REQUIRED_PYTHON"* ]]; then
    echo "âŒ Pythonç‰ˆæœ¬ä¸åŒ¹é…: å½“å‰$PYTHON_VERSION, è¦æ±‚$REQUIRED_PYTHON"
    exit 1
fi

# 2. æ£€æŸ¥Nodeç‰ˆæœ¬
NODE_VERSION=$(node --version | cut -d'v' -f2)
REQUIRED_NODE="18.18"
if [[ "$NODE_VERSION" != "$REQUIRED_NODE"* ]]; then
    echo "âŒ Nodeç‰ˆæœ¬ä¸åŒ¹é…: å½“å‰$NODE_VERSION, è¦æ±‚$REQUIRED_NODE"
    exit 1
fi

# 3. æ£€æŸ¥å¿…éœ€çš„ç¯å¢ƒå˜é‡
REQUIRED_ENV_VARS=(
    "DEEPSEEK_API_KEY"
    "DATABASE_URL"
    "REDIS_URL"
    "SECRET_KEY"
    "TENCENT_SMS_SECRET_ID"
    "TENCENT_SMS_SECRET_KEY"
)

for var in "${REQUIRED_ENV_VARS[@]}"; do
    if [ -z "${!var}" ]; then
        echo "âŒ ç¼ºå°‘ç¯å¢ƒå˜é‡: $var"
        exit 1
    fi
done

# 4. æ£€æŸ¥ä¾èµ–ç‰ˆæœ¬
echo "ğŸ“¦ éªŒè¯ä¾èµ–ç‰ˆæœ¬..."
cd backend && poetry check
if [ $? -ne 0 ]; then
    echo "âŒ åç«¯ä¾èµ–ç‰ˆæœ¬å†²çª"
    exit 1
fi

cd ../frontend && yarn check --integrity
if [ $? -ne 0 ]; then
    echo "âŒ å‰ç«¯ä¾èµ–å®Œæ•´æ€§æ£€æŸ¥å¤±è´¥"
    exit 1
fi

# 5. æ£€æŸ¥æœåŠ¡è¿é€šæ€§
echo "ğŸ”— æ£€æŸ¥æœåŠ¡è¿é€šæ€§..."
python -c "
import psycopg2
import redis
try:
    # æµ‹è¯•æ•°æ®åº“è¿æ¥
    conn = psycopg2.connect('$DATABASE_URL')
    conn.close()
    print('âœ… æ•°æ®åº“è¿æ¥æ­£å¸¸')
    
    # æµ‹è¯•Redisè¿æ¥
    r = redis.from_url('$REDIS_URL')
    r.ping()
    print('âœ… Redisè¿æ¥æ­£å¸¸')
except Exception as e:
    print(f'âŒ æœåŠ¡è¿æ¥å¤±è´¥: {e}')
    exit(1)
"

echo "âœ… ç¯å¢ƒä¸€è‡´æ€§éªŒè¯å…¨éƒ¨é€šè¿‡!"
