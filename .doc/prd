npx @apidevtools/swagger-parser validate api-specification.yaml
if [ $? -ne 0 ]; then
    echo "âŒ é”™è¯¯: OpenAPIè§„èŒƒæ–‡ä»¶æ ¼å¼ä¸æ­£ç¡®"
    exit 1
fi

# 3. å¯åŠ¨åç«¯æœåŠ¡
