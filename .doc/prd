# åœºæ™¯åŒ–è°ƒç”¨ç¤ºä¾‹
async def adaptive_temperature_call(scenario: str, prompt: str):
    """æ ¹æ®åœºæ™¯è‡ªåŠ¨é€‰æ‹©åˆé€‚çš„temperature"""
    setting = TEMPERATURE_SETTINGS.get(scenario, {"temperature": 1.0})
    
    return await deepseek_client.chat_completion(
        messages=[{"role": "user", "content": prompt}],
        model="deepseek-chat",
        temperature=setting["temperature"],
        max_tokens=2000
    )
```

#### deepseek-reasoneræ¨ç†æ¨¡å‹ä¸“ç”¨åŠŸèƒ½

```python
async def complex_reasoning_analysis(student_errors: dict):
    """ä½¿ç”¨deepseek-reasonerè¿›è¡Œå¤æ‚é”™è¯¯åˆ†æ"""
    response = await deepseek_client.chat_completion(
        messages=[{
            "role": "system",
            "content": "ä½ æ˜¯è‹±è¯­å­¦ä¹ è¯Šæ–­ä¸“å®¶ã€‚è¯·æ·±åº¦åˆ†æå­¦ç”Ÿçš„é”™è¯¯æ¨¡å¼ï¼Œæä¾›ç³»ç»Ÿæ€§æ”¹è¿›æ–¹æ¡ˆã€‚"
        }, {
            "role": "user", 
            "content": f"è¯·åˆ†æä»¥ä¸‹å­¦ç”Ÿé”™è¯¯æ•°æ®ï¼š{json.dumps(student_errors, ensure_ascii=False)}"
        }],
        model="deepseek-reasoner",  # ä½¿ç”¨æ¨ç†æ¨¡å‹
        # æ³¨æ„ï¼šdeepseek-reasonerä¸æ”¯æŒtemperatureå‚æ•°
        max_tokens=32000,  # æ”¯æŒæ›´é•¿çš„æ¨ç†è¾“å‡º
        stream=False
    )
    
    # å¤„ç†ç‰¹æ®Šçš„æ¨ç†å“åº”æ ¼å¼
    message = response.choices[0].message
    reasoning_content = getattr(message, 'reasoning_content', '')  # æ€ç»´è¿‡ç¨‹
    final_answer = message.content  # æœ€ç»ˆç­”æ¡ˆ
    
    return {
        "reasoning_process": reasoning_content,  # åˆ†æè¿‡ç¨‹ï¼ˆå¯é€‰æ‹©æ˜¯å¦å±•ç¤ºç»™ç”¨æˆ·ï¼‰
        "final_analysis": final_answer,         # æœ€ç»ˆåˆ†æç»“æœ
        "confidence": calculate_confidence(response)  # ç½®ä¿¡åº¦è¯„ä¼°
    }

# å¤šè½®å¯¹è¯æ—¶çš„æ³¨æ„äº‹é¡¹
async def multi_turn_reasoning(conversation_history: list):
    """deepseek-reasonerçš„å¤šè½®å¯¹è¯å¤„ç†"""
    # é‡è¦ï¼šåªä¼ é€’æœ€ç»ˆç­”æ¡ˆï¼Œä¸ä¼ é€’thinkingå†…å®¹
    filtered_history = []
    for msg in conversation_history:
        if msg['role'] == 'assistant':
            # åªä¿ç•™contentï¼Œä¸ä¿ç•™reasoning_content
            filtered_history.append({
                'role': 'assistant',
                'content': msg.get('content', '')
            })
        else:
            filtered_history.append(msg)
    
    return await deepseek_client.chat_completion(
        messages=filtered_history,
        model="deepseek-reasoner"
    )
```

#### æœ€æ–°å®šä»·ä¿¡æ¯å’Œæˆæœ¬è®¡ç®—

```python
# 2025å¹´æœ€æ–°DeepSeekå®šä»·ï¼ˆç¾å…ƒï¼‰
DEEPSEEK_PRICING = {
    "deepseek-chat": {
        "input_cache_hit": 0.07,     # ç¼“å­˜å‘½ä¸­æ—¶è¾“å…¥ä»·æ ¼/M tokens
        "input_cache_miss": 0.27,    # ç¼“å­˜æœªå‘½ä¸­æ—¶è¾“å…¥ä»·æ ¼/M tokens  
        "output": 1.10,              # è¾“å‡ºä»·æ ¼/M tokens
        "discount_hours": "16:30-00:30 UTC",  # ä¼˜æƒ æ—¶æ®µ
        "discount_rate": 0.5         # ä¼˜æƒ æ—¶æ®µ50%æŠ˜æ‰£
    },
    "deepseek-reasoner": {
        "input_cache_hit": 0.14,     # ç¼“å­˜å‘½ä¸­æ—¶è¾“å…¥ä»·æ ¼/M tokens
        "input_cache_miss": 0.55,    # ç¼“å­˜æœªå‘½ä¸­æ—¶è¾“å…¥ä»·æ ¼/M tokens
        "output": 2.19,              # è¾“å‡ºä»·æ ¼/M tokensï¼ˆåŒ…å«æ€ç»´é“¾ï¼‰
        "discount_hours": "16:30-00:30 UTC",  # ä¼˜æƒ æ—¶æ®µ
        "discount_rate": 0.75        # ä¼˜æƒ æ—¶æ®µ75%æŠ˜æ‰£
    }
}

# Tokenè®¡ç®—è§„åˆ™
TOKEN_CALCULATION = {
    "english_char_ratio": 0.3,   # 1è‹±æ–‡å­—ç¬¦â‰ˆ0.3 tokens
    "chinese_char_ratio": 0.6,   # 1ä¸­æ–‡å­—ç¬¦â‰ˆ0.6 tokens
    "min_cache_unit": 64         # ç¼“å­˜æœ€å°å•ä½64 tokens
}

def calculate_cost_estimate(text: str, model: str, is_input: bool = True, cache_hit: bool = False):
    """ç²¾ç¡®è®¡ç®—APIè°ƒç”¨æˆæœ¬"""
    # Tokenæ•°é‡ä¼°ç®—
    english_chars = sum(1 for c in text if c.isascii())
    chinese_chars = len(text) - english_chars
    
    estimated_tokens = (
        english_chars * TOKEN_CALCULATION["english_char_ratio"] +
        chinese_chars * TOKEN_CALCULATION["chinese_char_ratio"]
    )
    
    # å‘ä¸Šå–æ•´åˆ°åƒä½
    tokens_in_thousands = math.ceil(estimated_tokens / 1000)
    
    # ä»·æ ¼è®¡ç®—
    pricing = DEEPSEEK_PRICING[model]
    if is_input:
        price_per_k = pricing["input_cache_hit"] if cache_hit else pricing["input_cache_miss"]
    else:
        price_per_k = pricing["output"]
    
    # ä¼˜æƒ æ—¶æ®µæ£€æŸ¥
    current_utc = datetime.utcnow().time()
    if is_discount_time(current_utc):
        price_per_k *= pricing["discount_rate"]
    
    cost_usd = (tokens_in_thousands / 1000) * price_per_k
    cost_cny = cost_usd * 7.3  # æ±‡ç‡è½¬æ¢
    
    return {
        "estimated_tokens": int(estimated_tokens),
        "cost_usd": round(cost_usd, 4),
        "cost_cny": round(cost_cny, 4),
        "cache_hit": cache_hit,
        "discount_applied": is_discount_time(current_utc)
    }

# å®é™…ä½¿ç”¨åœºæ™¯çš„æˆæœ¬ä¼°ç®—
USAGE_COST_ESTIMATES = {
    "level_test": {
        "model": "deepseek-chat",
        "input_tokens": 1500,
        "output_tokens": 2000,
        "cost_cny": "Â¥0.15-0.20",
        "frequency": "ä¸€æ¬¡æ€§ï¼ˆæŠ¥åæ—¶ï¼‰"
    },
    "training_questions": {
        "model": "deepseek-chat", 
        "input_tokens": 300,
        "output_tokens": 800,
        "cost_cny": "Â¥0.03-0.05",
        "frequency": "æ¯å¤©10-15æ¬¡"
    },
    "error_analysis": {
        "model": "deepseek-chat",
        "input_tokens": 200,
        "output_tokens": 400,
        "cost_cny": "Â¥0.01-0.02", 
        "frequency": "æ¯å°æ—¶20-30æ¬¡"
    },
    "deep_reasoning": {
        "model": "deepseek-reasoner",
        "input_tokens": 500,
        "output_tokens": 1500,
        "cost_cny": "Â¥0.08-0.12",
        "frequency": "å¤æ‚åˆ†ææ—¶ä½¿ç”¨"
    }
}
```

#### ä¸Šä¸‹æ–‡ç¼“å­˜ï¼ˆContext Cachingï¼‰é«˜çº§åŠŸèƒ½

```python
class ContextCacheManager:
    """ä¸Šä¸‹æ–‡ç¼“å­˜ç®¡ç†å™¨"""
    
    def __init__(self):
        self.cache_templates = {
            "system_prompts": {},     # ç³»ç»Ÿæç¤ºè¯ç¼“å­˜
            "question_banks": {},     # é¢˜åº“æ¨¡æ¿ç¼“å­˜
            "analysis_contexts": {}   # åˆ†æä¸Šä¸‹æ–‡ç¼“å­˜
        }
    
    def build_cached_messages(self, template_type: str, dynamic_content: str):
        """æ„å»ºå¸¦ç¼“å­˜çš„æ¶ˆæ¯"""
        # è·å–å¯ç¼“å­˜çš„æ¨¡æ¿å†…å®¹
        cached_template = self.cache_templates[template_type]
        
        messages = [
            {
                "role": "system",
                "content": cached_template["system_prompt"],
                "cache_control": {"type": "ephemeral"}  # å¯ç”¨ç¼“å­˜
            },
            {
                "role": "user", 
                "content": cached_template["context"] + "\n\n" + dynamic_content
            }
        ]
        return messages
    
    async def cached_api_call(self, template_type: str, user_input: str, **kwargs):
        """ä½¿ç”¨ç¼“å­˜çš„APIè°ƒç”¨"""
        messages = self.build_cached_messages(template_type, user_input)
        
        response = await deepseek_client.chat_completion(
            messages=messages,
            **kwargs
        )
        
        # è®°å½•ç¼“å­˜å‘½ä¸­æƒ…å†µ
        usage = response.usage
        cache_hit_tokens = getattr(usage, 'cache_hit_tokens', 0)
        total_input_tokens = usage.prompt_tokens
        cache_hit_rate = cache_hit_tokens / total_input_tokens if total_input_tokens > 0 else 0
        
        return {
            "response": response,
            "cache_hit_rate": cache_hit_rate,
            "cost_saved": calculate_cache_savings(cache_hit_tokens)
        }

# ç¼“å­˜æ¨¡æ¿ç¤ºä¾‹
CACHE_TEMPLATES = {
    "level_test": {
        "system_prompt": """ä½ æ˜¯ä¸€ä½è‹±è¯­å››çº§æµ‹è¯•ä¸“å®¶ã€‚ä½ çš„ä»»åŠ¡æ˜¯æ ¹æ®å­¦ç”Ÿçš„è‡ªæˆ‘è¯„å®šç»“æœï¼Œç”Ÿæˆä¸ªæ€§åŒ–çš„æ°´å¹³æµ‹è¯•é¢˜ç›®ã€‚

æµ‹è¯•æ ‡å‡†ï¼š
- å¬åŠ›èƒ½åŠ›è¯„ä¼°ï¼ˆ15%æƒé‡ï¼‰
- å£è¯­èƒ½åŠ›è¯„ä¼°ï¼ˆ15%æƒé‡ï¼‰  
- é˜…è¯»èƒ½åŠ›è¯„ä¼°ï¼ˆ20%æƒé‡ï¼‰
- å†™ä½œèƒ½åŠ›è¯„ä¼°ï¼ˆ20%æƒé‡ï¼‰
- ç¿»è¯‘èƒ½åŠ›è¯„ä¼°ï¼ˆ15%æƒé‡ï¼‰
- è¯æ±‡é‡æµ‹è¯•ï¼ˆ10%æƒé‡ï¼‰
- è¯­æ³•åŸºç¡€ï¼ˆ5%æƒé‡ï¼‰

é¢˜ç›®è¦æ±‚ï¼š
1. éš¾åº¦åˆ†å¸ƒï¼šåŸºç¡€é¢˜40%ï¼Œä¸­ç­‰é¢˜35%ï¼ŒæŒ‘æˆ˜é¢˜25%
2. æ€»æ—¶é•¿æ§åˆ¶åœ¨30åˆ†é’Ÿå†…
3. é¢˜ç›®ç±»å‹å¤šæ ·åŒ–ï¼Œæ¶µç›–å„ä¸ªè¯„ä¼°ç»´åº¦
4. ç­”æ¡ˆè§£æè¯¦ç»†ï¼Œæœ‰åŠ©äºåç»­åˆ†æ

è¯·ä¸¥æ ¼æŒ‰ç…§ä»¥ä¸‹JSONæ ¼å¼è¾“å‡º...""",
        "context": "è¿™æ˜¯æ°´å¹³æµ‹è¯•ç”Ÿæˆçš„æ ‡å‡†ä¸Šä¸‹æ–‡ï¼ŒåŒ…å«è¯¦ç»†çš„å‡ºé¢˜è§„èŒƒå’Œè¯„åˆ†æ ‡å‡†..."
    }
}
