    "basic": {"amount": 10, "bonus": 0, "description": "åŸºç¡€å¥—é¤ï¼Œé€‚åˆå°è¯•ä½“éªŒ"},
    "standard": {"amount": 20, "bonus": 2, "description": "æ ‡å‡†å¥—é¤ï¼Œèµ é€2å…ƒï¼Œé€‚åˆæ—¥å¸¸ä½¿ç”¨"},
    "premium": {"amount": 30, "bonus": 5, "description": "ä¼˜æƒ å¥—é¤ï¼Œèµ é€5å…ƒï¼Œé€‚åˆé‡åº¦ä½¿ç”¨"},
    "advance": {"amount": 50, "bonus": 10, "description": "é«˜çº§å¥—é¤ï¼Œèµ é€10å…ƒï¼Œé€‚åˆé•¿æœŸå­¦ä¹ "},
